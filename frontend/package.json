{
  "name": "frontend",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "fix": "biome check . --apply",
    "lint": "biome check .",
    "preview": "npm run build && wrangler pages dev ./build/client",
    "deploy": "npm run build && wrangler pages deploy ./build/client",
    "typecheck": "tsc",
    "generate-api": "openapi-generator-cli generate -i ../klabis-api-spec.yaml -g typescript-fetch -o ./app/api/ --skip-validate-spec --additional-properties=useSingleRequestParameter=true,withoutPrefixEnums=true,enumNameSuffix=,withSeparateModelsAndApi=true,apiPackage=server,modelPackage=model --openapi-normalizer REFACTOR_ALLOF_WITH_PROPERTIES_ONLY=true"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-toast": "^1.1.5",
    "@remix-run/cloudflare": "^2.11.2",
    "@remix-run/cloudflare-pages": "^2.11.2",
    "@remix-run/express": "^2.11.2",
    "@remix-run/react": "^2.11.2",
    "@remix-run/serve": "^2.11.2",
    "@tanstack/react-table": "^8.20.5",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.0",
    "cmdk": "^1.0.0",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "date-fns": "^3.6.0",
    "express": "4.19.2",
    "isbot": "5.1.0",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.439.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.1",
    "react-phone-number-input": "^3.4.5",
    "remix-auth": "^3.7.0",
    "remix-auth-oauth2": "^2.3.0",
    "remix-utils": "7.5.0",
    "tailwind-merge": "2.2.1",
    "tailwindcss-animate": "1.0.7",
    "validator": "^13.11.0",
    "web-oidc": "^0.0.19",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@openapitools/openapi-generator-cli": "^2.13.5",
    "@remix-run/dev": "^2.11.2",
    "@tailwindcss/typography": "0.5.10",
    "@types/react": "18.2.56",
    "@types/react-dom": "18.2.19",
    "autoprefixer": "10.4.17",
    "dotenv-cli": "7.3.0",
    "postcss": "8.4.35",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "vite": "5.1.3",
    "vite-env-only": "2.2.0",
    "vite-tsconfig-paths": "4.3.1",
    "wrangler": "^3.75.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
